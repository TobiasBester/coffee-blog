{
  "name": "cms-sanity",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "copy:sanity": "copy .env.local ./studio/.env.development",
    "start:sanity": "cd studio && sanity start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^2.8.0",
    "@sanity/image-url": "^0.140.19",
    "classnames": "^2.2.6",
    "date-fns": "^2.15.0",
    "next": "^12.0.8",
    "next-sanity": "^0.4.0",
    "nextjs-progressbar": "0.0.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.3.6",
    "react-icons": "^4.3.1",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sanity/cli": "^2.27.0",
    "lerna": "^4.0.0",
    "postcss-preset-env": "^7.2.3",
    "tailwindcss": "^3.0.17"
  }
}
